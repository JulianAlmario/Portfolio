---
import BarNav from "./BarNav.astro";
import HeaderSideBar from "./HeaderSideBar.astro";


const headerType:{title:string, url:string}[] = [
  {
    title: "Home",
    url: "/",
  },
   {
    title: "About",
    url: "/about",
  },
  {
    title: "Projects",
    url: "/projects",
  },
